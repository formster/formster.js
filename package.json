{
  "name": "signupsio.js",
  "version": "0.3.0",
  "description": "Client-side Form Tracking for Signups.io",
  "keywords": [
    "signups.io",
    "form tracking"
  ],
  "main": "./src/signupsio.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "browserify src/signupsio.js -s Signupsio > build/signupsio.js",
    "build-prod": "browserify src/signupsio.js -s Signupsio | uglifyjs > build/signupsio.min.js",
    "build": "npm run build-dev & npm run build-prod",
    "watch": "watchify src/signupsio.js -o build/signupsio.js -s Signupsio"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/signupsio/signupsio.js.git"
  },
  "homepage": "http://github.com/signupsio/signupsio.js",
  "author": "trey.griffith@gmail.com",
  "license": "MIT",
  "dependencies": {
    "qs": "^0.6.6",
    "randomstring": "^1.0.3",
    "reqwest": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "~2.35.2",
    "uglifyjs": "~2.3.6",
    "watchify": "^0.6.3"
  }
}
